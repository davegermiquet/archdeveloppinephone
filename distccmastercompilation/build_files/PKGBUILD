pkgname=Zandronum
pkgver=3.1
pkgrel=1
pkgdesc='Zandronum Doom Like Game'
url=https://github.com/ptitSeb/zandronum
arch=('aarch64')
license=('Sleepycat License')

build() {
   git clone https://github.com/ptitSeb/zandronum.git
   cd zandronum
   git checkout -b diffBranch 9156afd
   cp /tmp/setup/GameCompiler.patch game-music-emu/
   git log
   cd game-music-emu/
   patch CMakeLists.txt < GameCompiler.patch
   cat CMakeLists.txt.rej
   cd ../
   mkdir build
   cd build
   cmake .. -DCMAKE_BUILD_TYPE=Release -DSERVERONLY=OFF -DRELEASE_WITH_DEBUG_FILE=OFF
   make -j6
} 

package() {
   cd zandronum/build
   make install
}